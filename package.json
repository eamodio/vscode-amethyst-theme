{
	"name": "amethyst-theme",
	"displayName": "Amethyst Themes",
	"description": "A set of very purple dark themes, based on the built-in Dark+ theme",
	"version": "2.6.0",
	"publisher": "eamodio",
	"engines": {
		"vscode": "^1.26.0"
	},
	"license": "SEE LICENSE IN LICENSE",
	"categories": [
		"Themes"
	],
	"keywords": [
		"purple",
		"violet",
		"dark"
	],
	"galleryBanner": {
		"color": "#7d16bf",
		"theme": "dark"
	},
	"icon": "images/icon.png",
	"homepage": "https://github.com/eamodio/vscode-amethyst-theme/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/eamodio/vscode-amethyst-theme/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eamodio/vscode-amethyst-theme.git"
	},
	"contributes": {
		"themes": [
			{
				"id": "amethyst-dark",
				"label": "Amethyst Dark",
				"path": "./themes/amethyst-dark-theme.json",
				"uiTheme": "vs-dark"
			},
			{
				"id": "amethyst-dark-hc",
				"label": "Amethyst Dark (Higher Contrast)",
				"path": "./themes/amethyst-dark-hc-theme.json",
				"uiTheme": "vs-dark"
			},
			{
				"id": "amethyst",
				"label": "Amethyst",
				"path": "./themes/amethyst-theme.json",
				"uiTheme": "vs-dark"
			}
		]
	},
	"scripts": {
		"pack": "git clean -df && vsce package",
		"pretty": "prettier --config .prettierrc --loglevel warn --write \"./**/*.{js,md,json}\"",
		"pub": "git clean -df && vsce publish"
	},
	"devDependencies": {
		"prettier": "2.0.2",
		"vsce": "1.74.0"
	}
}
